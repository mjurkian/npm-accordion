{
  "extends": [
    "airbnb"
  ],
  "parser" : "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2016,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest": true
  },
  "rules" : {
    "linebreak-style": 0,
    "comma-dangle" : 0
  }
}
